{
  "commands": {
    "Reset Site": {
      "fileName": "powershell.exe",
      "workingDirectory": ".",
      "arguments": "-Command \"Remove-Item 'src\\Lombiq.OSOCE.NuGet.Web\\App_Data' -Recurse -ErrorAction Ignore;Write-Output 'Successfully reset website.'\""
    },
    "Update Lombiq NuGet packages": {
      "fileName": "powershell.exe",
      "workingDirectory": ".",
      "arguments": "-Command \"Update-VisualStudioSolutionNuGetPackages -Path (Get-Location).Path -ProjectNameFilter 'Lombiq.*' -PackageNameFilter 'Lombiq.*';Write-Output 'Finished updating packages.'\""
    }
  }
}
