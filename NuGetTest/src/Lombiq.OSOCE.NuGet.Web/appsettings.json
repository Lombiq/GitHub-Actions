{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OrchardCore": {
    "OrchardCore_DataProtection_Azure": {
      "ConnectionString": "UseDevelopmentStorage=true",
      "ContainerName": "osocedataprotection"
    },
    "OrchardCore_Media_Azure": {
      "ConnectionString": "UseDevelopmentStorage=true",
      "ContainerName": "osocemedia"
    },
    "Lombiq_Hosting_Tenants_FeaturesGuard": {
      "AlwaysEnabledFeaturesOptions": {
        "AlwaysEnabledFeatures": [
          "DotNest.Hosting.Tenants",
          "DotNest.TenantsAdmin.Subtenant",
          "Lombiq.Hosting.Tenants.Admin.Login",
          "OrchardCore.Roles",
          "OrchardCore.Users"
        ]
      }
    },
    "OrchardCore_Media": {
      "AllowedFileExtensions": [
        ".3gp",
        ".avi",
        ".css",
        ".doc",
        ".docx",
        ".eot",
        ".gif",
        ".html",
        ".ico",
        ".jfif",
        ".jpeg",
        ".jpg",
        ".js",
        ".liquid",
        ".m4a",
        ".m4v",
        ".mov",
        ".mp3",
        ".mp4",
        ".mpg",
        ".odt",
        ".ogg",
        ".ogv",
        ".pdf",
        ".png",
        ".pps",
        ".ppsx",
        ".ppt",
        ".pptx",
        ".psd",
        ".svg",
        ".ttf",
        ".wav",
        ".wmv",
        ".woff",
        ".xls",
        ".xlsx",
        ".zip"
      ]
    }
  }
}
